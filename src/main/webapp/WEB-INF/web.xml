<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>BookController</servlet-name>
		<display-name>BookController</display-name>
		<description></description>
		<servlet-class>com.jdbc.controller.BookController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>BookController</servlet-name>
		<url-pattern>/books/*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>books.jsp</welcome-file>
		<!-- <welcome-file>add-book.jsp</welcome-file> -->
		<!-- <welcome-file>edit-book.jsp</welcome-file> -->
	</welcome-file-list>

</web-app>